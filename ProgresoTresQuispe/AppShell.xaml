<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ProgresoTresQuispe.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:ProgresoTresQuispe.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="ProgresoTresQuispe">

    <TabBar>
        <Tab Title="Inicio" Icon="home.png">
            <ShellContent
                Title="MainPage"
                ContentTemplate="{DataTemplate views:MainPage}"
                Route="MainPage" />
        </Tab>

        <Tab Title="Registrar" Icon="add.png">
            <ShellContent
                Title="Registro"
                ContentTemplate="{DataTemplate views:RegistroPage}"
                Route="RegistroPage" />
        </Tab>
        <Tab Title="Logs">
            <ShellContent ContentTemplate="{DataTemplate views:LogsPage}" />
        </Tab>

    </TabBar>

</Shell>
